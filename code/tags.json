{
    "post": {
        "tag": "div",
        "attrs": {
            "data-testid": "post-container"
        }
    },

    "title-container": {
        "ancestor": "post",
        "tag": "div",
        "attrs": {
            "data-adclicklocation": "title"
        }
    },

    "title": {
        "ancestor": "title-container",
        "tag": "h3",
        "attrs": {
            "class": "_eYtD2XCVieq6emjKBH3m"
        }
    },

    "time": {
        "ancestor": "post",
        "tag": "span",
        "attrs": {
            "data-testid": "post_timestamp",
            "data-click-id": "timestamp"
        }
    },

    "comment-container": {
        "ancestor": "post",
        "tag": "a",
        "attrs": {
            "data-adclicklocation": "comments",
            "data-click-id": "comments",
            "data-test-id": "comments-page-link-num-comments"
        }
    },

    "comments": {
        "ancestor": "comment-container",
        "tag": "span",
        "attrs": {
            "class": "FHCV02u6Cp2zYL0fhQPsO"
        }
    },

    "body": {
        "ancestor": "post",
        "tag": "a",
        "attrs": {
            "data-click-id": "body"
        }
    },

    "body-text": {
        "ancestor": "body",
        "tag": "div",
        "attrs": {
            "data-click-id": "text",
            "class": "Chtkt3BCZQruf0LtmFg2c"
        }
    },

    "media": {
        "ancestor": "body",
        "tag": "div",
        "attrs": {
            "data-click-id": "media"
        }
    },

    "video": {
        "ancestor": "media",
        "tag": ["iframe", "video"],
        "attrs": {
            "class": "media-element"
        }
    },

    "image": {
        "ancestor": "media",
        "tag": "img",
        "attrs": {
            "alt": "Post image"
        }
    }
}
